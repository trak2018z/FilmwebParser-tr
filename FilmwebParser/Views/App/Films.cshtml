@model IEnumerable<FilmwebParser.Models.Film>
@{
    ViewBag.Title = "Strona główna";
}

<div class="row">
    <div class="col-md-6">
        <h2>Tu wklej link</h2>
        <form>
            <div class="form-group">
                <input class="form-control" />
            </div>
            <div><input type="submit" value="Rozpocznij przetwarzanie" class="btn btn-success" /></div>
        </form>
    </div>
    <div class="col-md-6">
        <h2>Historia</h2>
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.Title: @item.DateAdded.ToString("d")</li>
            }
        </ul>
    </div>
</div>